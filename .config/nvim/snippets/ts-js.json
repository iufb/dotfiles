{
    "Print to console": {
        "prefix": "cl",
        "body": [
            "console.log($1)"
        ],
        "description": "Log output to console"
    },
    "Css module import": {
        "prefix": "mcss",
        "body": [
            "import styles from './$1'"
        ],
        "description": "snippet for css module"
    },
    "i18n": {
        "prefix": "n",
        "body": [
            "t('$1')"
        ],
        "description": "snippet for  next-intl"
    },
    "RN Style": {
        "prefix": "nst",
        "body": [
            "style={[styles.$1]}"
        ],
        "description": "snippet for  RN style prop"
    },
    "RN StyleSheet": {
        "prefix": "sh",
        "body": [
            "const styles = StyleSheet.create({",
            "   container: {",
            "$1",
            "   },",
            "$2",
            "})"
        ],
        "description": "snippet for  next-intl"
    },
    "RN Component": {
        "prefix": "rnfc",
        "body": [
            "import { StyleSheet,View } from 'react-native' ",
            "export const $1 = () => {",
            "return (",
            "<View>",
            "$2",
            "</View>",
            ");",
            "};",
            "const styles = StyleSheet.create({})"
        ]
    },
    "NextJS Page": {
        "prefix": "npage",
        "body": [
            "import React from 'react';",
            "",
            "export default function $1() {",
            "  return (",
            "    <section>",
            "      $2",
            "    </section>",
            "  );",
            "}"
        ],
        "description": "Next.js page component "
    },
    " Arrow Func": {
        "prefix": "rfn",
        "body": [
            "import React from 'react';",
            "",
            "export const ${TM_FILENAME_BASE} = () => {",
            "  return (",
            "      $2",
            "  );",
            "};"
        ],
        "description": " arrow function and named export"
    }
}
